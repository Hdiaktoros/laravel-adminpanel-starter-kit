{
    "name": "vrkansagara/laravel-api",
    "description": "Laravel API",
    "keywords": ["framework", "laravel"],
    "license": "BSD-3-Clause",
    "type": "project",
    "authors": [
        {
            "name": "Vallabh Kansagara",
            "email": "vrkansagara@gmail.com",
            "homepage": "https://vrkansagara.in",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "vrkansagara@gmail.com",
        "irc": "irc://irc.freenode.org/composer",
        "issues":"https://github.com/vrkansagara/laravel-api/issues",
        "source":"https://github.com/vrkansagara/laravel-api/releases"
    },
    "require": {
        "php": "^7.1.3",
        "arcanedev/laravel-settings": "^4.0",
        "arcanedev/log-viewer": "^4.6",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.5",
        "bosnadev/repositories": " 0.*",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "graham-campbell/flysystem": "^5.1",
        "laravel/cashier": "^8.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/passport": "^7.0",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "^1.0",
        "pusher/pusher-php-server": "~3.0",
        "spatie/laravel-activitylog": "^3.1",
        "spatie/laravel-analytics": "^3.6",
        "spatie/laravel-cors": "^1.3",
        "spatie/laravel-permission": "^2.23",
        "tylercd100/lern": "^4.5"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "laravel/telescope": "^0.1.6",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-coding-standard": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "database/migrations/traits"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "cs-check": "./vendor/bin/phpcs",
        "cs-fix": "./vendor/bin/phpcbf"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
